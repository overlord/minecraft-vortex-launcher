<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style x:Key="x_top_label" TargetType="{x:Type TextBlock}">
		<Setter Property="Margin" Value="0" />
		<Setter Property="TextAlignment" Value="Left" />
	</Style>
	<Style x:Key="x_label" TargetType="{x:Type TextBlock}">
		<Setter Property="Margin" Value="0,4,0,0" />
		<Setter Property="TextAlignment" Value="Left" />
	</Style>
	<Style x:Key="x_field" TargetType="{x:Type TextBox}">
		<Setter Property="Margin" Value="0" />
	</Style>
	<Style x:Key="x_vbutton" TargetType="{x:Type Button}">
		<Setter Property="Margin" Value="0,4,0,0" />
	</Style>
	<Thickness x:Key="x-mvt">0</Thickness>
	<Thickness x:Key="x-mv">0,4,0,0</Thickness>
</ResourceDictionary>
